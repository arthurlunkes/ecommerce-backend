{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 80,
  "trailingComma": "all",
  "embeddedLanguageFormatting": "auto",
  "arrowParens": "always",
  "endOfLine": "lf",
  "useTabs": false,
  "singleAttributePerLine": true,
  "bracketSpacing": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false
}
